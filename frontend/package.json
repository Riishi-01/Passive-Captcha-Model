{
  "name": "passive-captcha-frontend",
  "version": "1.0.0",
  "description": "Modern frontend architecture for Passive CAPTCHA Admin Dashboard",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "lint": "eslint . --ext .js,.ts,.vue,.jsx,.tsx",
    "lint:fix": "eslint . --ext .js,.ts,.vue,.jsx,.tsx --fix",
    "format": "prettier --write .",
    "typecheck": "tsc --noEmit",
    "analyze": "vite-bundle-analyzer",
    "deploy": "npm run build && vercel --prod"
  },
  "dependencies": {
    "vue": "^3.4.0",
    "vue-router": "^4.2.5",
    "pinia": "^2.1.7",
    "axios": "^1.6.2",
    "@headlessui/vue": "^1.7.16",
    "@heroicons/vue": "^2.0.18",
    "@vueuse/core": "^10.5.0",
    "@vueuse/head": "^2.0.0",
    "chart.js": "^4.4.0",
    "vue-chartjs": "^5.2.0",
    "date-fns": "^2.30.0",
    "tailwindcss": "^3.3.6",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.10",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.32",
    "lucide-vue-next": "^0.294.0",
    "vue-toastification": "^2.0.0-rc.5",
    "socket.io-client": "^4.7.4",
    "jwt-decode": "^4.0.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.5.2",
    "vite": "^5.0.10",
    "vitest": "^1.0.4",
    "@vitest/ui": "^1.0.4",
    "@vue/test-utils": "^2.4.3",
    "jsdom": "^23.0.1",
    "typescript": "^5.3.3",
    "vue-tsc": "^1.8.25",
    "@types/node": "^20.10.5",
    "eslint": "^8.56.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "eslint-plugin-vue": "^9.19.2",
    "prettier": "^3.1.1",
    "@vue/eslint-config-prettier": "^9.0.0",
    "vite-bundle-analyzer": "^0.7.0",
    "vercel": "^32.7.2",
    "@testing-library/vue": "^8.0.1",
    "@testing-library/jest-dom": "^6.1.6"
  },
  "keywords": [
    "passive-captcha",
    "admin-dashboard",
    "vue3",
    "typescript",
    "tailwindcss",
    "vite",
    "modern-frontend"
  ],
  "author": "Passive CAPTCHA Team",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
} 