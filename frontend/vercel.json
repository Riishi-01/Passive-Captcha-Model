{
  "headers": [
    {
      "source": "/passive-captcha.js",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Cache-Control", 
          "value": "public, max-age=31536000"
        },
        {
          "key": "Content-Type",
          "value": "application/javascript"
        }
      ]
    },
    {
      "source": "/src/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/demo",
      "destination": "/demo/index.html"
    },
    {
      "source": "/passive-captcha.js",
      "destination": "/src/passive-captcha.js"
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false
} 